<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Group?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.StackPane?>


<AnchorPane xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/9" fx:controller="controller.ViewMeme">
   <children>
      <BorderPane prefHeight="800.0" prefWidth="1000.0">
         <center>
            <StackPane fx:id="pane" prefHeight="690.0" prefWidth="1000.0" BorderPane.alignment="CENTER">
               <children>
                  <Group fx:id="group">
                     <children>
                        <ImageView fx:id="imgView" fitHeight="700.0" fitWidth="980.0" layoutX="45.0" layoutY="-14.0" pickOnBounds="true" preserveRatio="true" />
                     </children>
                  </Group>
               </children>
            </StackPane>
         </center>
         <top>
            <MenuBar prefHeight="39.0" prefWidth="464.0" BorderPane.alignment="CENTER">
              <menus>
                <Menu mnemonicParsing="false" text="File">
                  <items>
                        <MenuItem mnemonicParsing="false" onAction="#fileOpen" text="Open" />
                    <MenuItem mnemonicParsing="false" onAction="#closeFunction" text="Close" />
                  </items>
                </Menu>
                  <Menu mnemonicParsing="false" text="Home">
                    <items>
                      <MenuItem mnemonicParsing="false" onAction="#goToWelcome" text="Welcome" />
                        <MenuItem mnemonicParsing="false" onAction="#goToMode" text="Mode" />
                    </items>
                  </Menu>
              </menus>
            </MenuBar>
         </top>
      </BorderPane>
   </children>
</AnchorPane>
